{
  "functions": {
    "source": "functions",
    "runtime": "nodejs18"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/scrape-silver",
        "function": "scrapeSilver"
      },
      {
        "source": "/api/scrape-gold",
        "function": "scrapeGold"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
